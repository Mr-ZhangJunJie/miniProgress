/* pages/orderFinish/index.wxss */
.container{
  background: #EAEAEA;
}
.header{
  width:100%;height:282rpx;background: #E53B3E;position: relative;
}
.goBack{
  width:40rpx;height:40rpx;position: absolute;left:32rpx;top:32rpx;
}
.headerOrder{
  width:706rpx;height:258rpx;position: absolute;top:118rpx;left:50%;margin-left:-353rpx;background: #ffffff;box-sizing: border-box;padding:60rpx 0 90rpx;
}
.headTitle{
  font-size:34rpx;text-align: center;line-height: 72rpx;
}
.headThank{
  font-size: 24rpx;text-align: center;color:#8E8D8D;
}

.orderDetail{
  width: 706rpx;margin:0 auto;margin-top:118rpx;box-shadow: 0 0 2px #EBEBEB;
}
.stopName{
  height:106rpx;box-sizing: border-box;padding:0 20rpx;line-height: 106rpx;
  background: #ffffff;
}
.stopName .left{
  float:left;color:#5A5A5A;
}
.stopName .left image{
  width:51rpx;height:51rpx;vertical-align: middle;margin-right:12rpx;
}
.stopName .right{
  float:right;
}
.stopName .right image{
  width:29rpx;height:32rpx;
}


.orderContent{
  width:706rpx;margin:0 auto;height:160rpx;padding:22rpx 12rpx;box-sizing: border-box;
  background: #F8F8F8;
}
.orderContent .left{
  float:left;
}
.orderContent .left>view{
  float:left;
}
.orderContent .left image{
  width:115rpx;height:115rpx;
}
.goodsList{
  margin-left:22rpx; 
}
.goodsList view:first-child{
  font-size: 26rpx;line-height: 38rpx;color:#484848;
}
.goodsList view:nth-child(2){
  font-size: 22rpx;line-height: 50rpx;color:#B1B1B1;
}
.goodsList view:nth-child(3){
  font-size: 18rpx;line-height: 30rpx;color:#B1B1B1;
}
.orderContent .right{
  float:right;font-size:24rpx; 
}


.costBox{
  width:706rpx;margin:0 auto;background: #ffffff;padding:0 22rpx;box-sizing: border-box;
}
.cost{
  height:138rpx;background: #ffffff;border-bottom: 1px dotted #E7E7E7;font-size: 26rpx;
}
.cost>view{
  height:69rpx;line-height: 69rpx;color:#414141;
}
.costName{
  float:left;
}
.costCount{
  float:right;
}

.totalMoney{
  height:92rpx;background: #ffffff;padding: 0 22rpx;box-sizing: border-box;
}
.totalMoneyRight{
  height:92rpx;line-height:92rpx;float:right;font-size:30rpx; 
}
.totalMoneyRight text{
  font-size: 26rpx;color:#F84341; 
}

.distribution{
  padding:20rpx 22rpx;width:706rpx;margin:0 auto;box-sizing: border-box;background: #ffffff;
  margin-top:30rpx;
}
.distributionAdress{
  height:48rpx;
}
.distributionAdress .left{
 float:left;font-size: 22rpx;color:#939393; margin-right:36rpx;
}
.distributionAdress .right view{
  height:24rpx;line-height: 24rpx;color:#161616;font-size: 22rpx;
}


.other{
  height:86rpx;line-height: 86rpx;font-size: 22rpx;
}
.other .left{
   float:left;color:#939393; margin-right:36rpx;
}